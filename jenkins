pipeline {
    agent any
    stages {
        stage ('anyip') {
            steps {
                echo "displaying any ip"
                sh "hostname -i"
            }
        }
        stage ('myip') {
            agent {
                label 'slave2'
            }
            steps {
                echo "displaying my ip"
                sh "hostname -i"
            }
        }
    }
}
