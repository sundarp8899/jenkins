pipeline {
    agent any
    stages {
        stage ('any ip') {
            steps {
                echo "displaying any machine ip"
                sh "hostname -i"
            }
        }
        stage ('myip') {
            agent {
                label 'slave2'
            }
            steps {
                echo "displaying my ip"
                sh "hostname -i"
            }
        }
        }

    }
}
